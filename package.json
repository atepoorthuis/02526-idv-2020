{
  "name": "02526-idv-2020",
  "description": "Class website for IDV 2020",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev --ext '.md .svelte'",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build --legacy --ext '.md .svelte'",
    "export": "npm run build:tailwind && sapper export --legacy --ext '.md .svelte'",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^2.0.5",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@snlab/sveltex-unified": "0.0.10",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "purgecss-from-html": "^2.0.3",
    "purgecss-from-svelte": "^2.0.2",
    "rollup": "^1.30.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-postcss": "^2.0.5",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "sapper": "^0.27.9",
    "sync-rpc": "^1.3.6",
    "svelte": "^3.18.0",
    "svelte-preprocess": "^3.3.0",
    "tailwindcss": "^1.1.4"
  }
}
